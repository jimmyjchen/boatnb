<div class="layout-newboat-form-page">
  <div class="intro">
    <h1>List Your Boat</h1>
    <h4>Boatnb lets you make money renting out your boat</h4>
  </div>
  <div class="row">
    <div class="col-xs-12 col-sm-6 offset-sm-3">
      <div class="boat-form">
        <%= simple_form_for @boat do |f| -%>
        <%= f.input :title %>
        <%= f.input :description %>
        <%= f.input :price %>
        <%= f.input :address %>
        <%= f.input :tag_list, as: :check_boxes, collection: ["Yatch", "Sailing", "Canoe", "Cruise", "Fishing Boat", "Speedy", "Family", "Romantic", "Engine-Power", "Men-Power"] %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.input :photo_1 %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.input :photo_2 %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.input :photo_3 %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.button :submit, "Create", class:"btn btn-create-account" %>
      </div>
      <% end %>
    </div>
  </div>
</div>





