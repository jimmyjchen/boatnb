<div class="layout-form-page">
  <img src="https://images.unsplash.com/photo-1454452176678-c0437432bba6?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1050&q=80" />
  <div class="row">
    <div class="col-xs-12 col-sm-6 offset-sm-3">

  <div class="boat-form">
    <%= simple_form_for @boat do |f| -%>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit %>
    <% end -%>
  </div>
</div>
</div>
</div>
